[Unit]
Description=uich.cc service
After=network.target
Requires=network.target

[Service]
ExecStart=/usr/local/bin/uwsgi /root/flask/uichcc.ini
ExecStop=/usr/local/bin/uwsgi --stop /root/flask/uichcc.pid
ExecReload=/usr/local/bin/uwsgi --reload /root/flask/uichcc.pid
Type=forking

[Install]
WantedBy=multi-user.target
